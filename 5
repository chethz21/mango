> db.students.aggregate([
{
$group: {
_id: null,
averageAge: { $avg: "$age" }
}}
])

> db.students.aggregate([
{
$group: {
_id: null,
minAge: { $min: "$age" }, 
maxAge: { $max: "$age" }
}}
])

> db.students.aggregate([
{
$group: {
_id: null,
allNames: { $push: "$name" }}
}
])

> db.students.aggregate([
{
$group: {
_id: null,
uniqueGrades: { $addToSet: "$grade" } }
}
])

> db.students.aggregate([
{
$group: {
_id: null,
totalStudents: { $sum: 1 }}
}
])

> db.students.aggregate([
{
$project: { 
name: 1,
averageScore: { $avg: "$scores" } }}])
