{student_id: 4, name: "Diana Prince", age: 21, grade: "C", scores: [65, 70, 75] },
{student_id: 5, name: "Eve Adams", age: 23, grade: "B", scores: [80, 85, 90] },
{student_id: 6, name: "Frank White", age: 22, grade: "A", scores: [88, 92, 96] },
